        <ClinVarAccession Accession="SCV000020152" DateUpdated="2017-12-15" DateCreated="2013-04-04" Type="SCV" Version="3" SubmitterName="OMIM" OrgID="3" OrganizationCategory="resource"/>
        <RecordStatus>current</RecordStatus>
        <Classification DateLastEvaluated="2001-06-01">
